#!/usr/bin/env perl
use warnings;
use strict;

BEGIN {
	foreach my $devdir ("$ENV{HOME}/git/geo-modules/Geo-GTFS/lib") {
		if (-d $devdir) {
			$ENV{USE_INLINE_TEMP_CACHE} = 1;
			require lib;
			import lib $devdir;
		}
	}
}

use Geo::GTFS::Realtime;

my $rt = Geo::GTFS::Realtime->new();
$rt->pull();

